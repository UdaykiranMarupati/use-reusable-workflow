{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",

  "regexManagers": [
    {
      "fileMatch": ["^\\.github/workflows/[^/]+\\.ya?ml$"],
      "matchStrings": ["uses: (?<depName>.*?)/.github/workflows/dev-reusable.yml@(?<currentValue>.*?)\\s"],
      "datasourceTemplate": "github-releases"
    }
  ],
  "enabled": true
}
